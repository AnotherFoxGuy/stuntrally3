//  black desert
hlms grass_emis pbs
{
    alpha_test greater 0.5
    cull_mode none
    // grass_wind true
    // vertex_colour true
    // diffuse_address_mode clamp
	two_sided true

    diffuse 0 0 0
    specular 0 0 0
    emissive 1 1 1
	fresnel_coeff 0.01
	roughness 0.1
	metalness 0.1
}

hlms grassZ10 pbs : grass_emis
{
	// emissive 0.3 0.3 0.3
    diffuse_map  grassZ10.png
    emissive_map grassZ10.png
