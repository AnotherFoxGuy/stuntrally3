hlms grass pbs
{
    alpha_test greater 0.5
    cull_mode none
    // grass_wind true
    // vertex_colour true
    // diffuse_address_mode clamp
	two_sided true

    diffuse 1 1 1
    specular 0.0 0.0 0.0
	fresnel_coeff 0.01
	roughness 0.4
	// metalness 0.1
}

