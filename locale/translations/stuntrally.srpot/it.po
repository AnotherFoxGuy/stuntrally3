#: :2224..Editor.Grasses.Channels.Text :2229..Editor.Grasses.Channels.Edit
#: :2234..Editor.Grasses.Channels.Slider
msgctxt "TipTerLHSm"
msgid "Height smooth range (transition)."
msgstr "Raggio di altezza smoothing (transizione)."

#. TIP  Terrain  blendmap 
#: data/gui/Editor.layout:1643 ..Editor.Layers.Blendmap.Blendmap.Text
#: :1647:1651..Editor.Layers.Blendmap.Blendmap.Slider
#: :1657..Editor.Layers.Blendmap.Blendmap.Text
#: :1661:1665..Editor.Layers.Blendmap.Blendmap.Slider
#: :1671..Editor.Layers.Blendmap.Blendmap.Text
#: :1675:1679..Editor.Layers.Blendmap.Blendmap.Slider
#: :1757..Editor.Layers.Blendmap.Noise.ProgressBar
msgctxt "TipTerLNoise"
msgid ""
"Multiplier for additional noise. +1 blends to next used layer, -1 to "
"previous, +2 to over next."
msgstr ""
"Moltiplicatore per rumore aggiuntivo. +1 si fonde al successivo layer usato, "
"-1 al precedente, +2 dopo il prossimo."

#. TIP  Terrain  blendmap 
#: data/gui/Editor.layout:1685 ..Editor.Layers.Blendmap.Blendmap.Check
msgctxt "TipTerNoiseOnly"
msgid ""
"Will ignore terrain angle and height ranges. Layer will only appear, if "
"other layer's noise multiplier blends to this."
msgstr ""
"Ignorerà angolo di terreno e gamme di altezza. Il layer apparirà soltanto, "
"se l'altro moltiplicatore di disturbo si fonde a questo."

#. TIP  Terrain  blendmap 
#: data/gui/Editor.layout:1539 ..Editor.Layers.Check
msgctxt "TipTestBlendmap"
msgid ""
"Use to test blendmap clarity. The color box shows this layer's color on "
"blendmap (is black when layer is off). Usually, a good blendmap needs to "
"have pure layer colors with little blending (dull, mixed colors). To achieve "
"that narrow/separate angle and height ranges and use high or low noise power."
msgstr ""
"Usata per testare la chiarezza del blendmap. La casella di colore indica il "
"colore del layer su blendmap (è nero quando il layer è spento). Di solito, "
"un buon blendmap deve avere colori di layer puri con poca miscelazione "
"(opaco, colori misti). Per ottenere angoli stretti/separati e la gamma di "
"altezza, usa alta o bassa capacità di disturbo."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3069 ..Editor.Vegetation.Surface.Surface.Edit
#: :3092..Editor.Vegetation.Surface.Surface.Text
msgctxt "TipDustInt"
msgid "Dust particles intensity."
msgstr "Intensità particelle di polvere."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3074 ..Editor.Vegetation.Surface.Surface.Edit
#: :3097..Editor.Vegetation.Surface.Surface.Text
msgctxt "TipDustSize"
msgid "Dust particles size."
msgstr "Dimensioni particelle di polvere."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3079 ..Editor.Vegetation.Surface.Surface.Edit
#: :3102..Editor.Vegetation.Surface.Surface.Text
msgctxt "TipMudInt"
msgid "Mud particles intensity."
msgstr "Intensità particelle di fango."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3084 ..Editor.Vegetation.Surface.Surface.Edit
#: :3107..Editor.Vegetation.Surface.Surface.Text
msgctxt "TipSmokeInt"
msgid "Smoke particles intensity."
msgstr "Intensità particelle di fumo."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3112 ..Editor.Vegetation.Surface.Surface.Text
msgctxt "TipTrailClr"
msgid "Tire trails color. R G B A components 0 to 1 (eg. 0.4 0.4 0.2 0.6)"
msgstr ""
"Colore delle scie di gomme. Elementi R G B A da 0 a 1 (es. 0.4 0.4 0.2 0.6)"

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3119 ..Editor.Vegetation.Surface.Surface.Combo
msgctxt "TipParType1"
msgid "Tire Particle type 1 (global for track)."
msgstr "Particella di pneumatici di tipo 1 (globale per la pista)."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3126 ..Editor.Vegetation.Surface.Surface.Combo
msgctxt "TipParType2"
msgid "Tire Particle type 2 (global for track)."
msgstr "Particella di pneumatici di tipo 2 (globale per la pista)."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3133 ..Editor.Vegetation.Surface.Surface.Combo
msgctxt "TipParType3"
msgid "Tire Particle type 3 (global for track)."
msgstr "Particella di pneumatici di tipo 3 (globale per la pista)."

#. TIP  Terrain  particles 
msgctxt "TipTireParType3"
msgid "Tire Particle type 3."
msgstr "Particelle ruote di tipo 3."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3159 ..Editor.Vegetation.Surface.Surface.Text :3192
#: data/gui/Game_Tweak.layout:325 ..Tweak.Surface.Text :332:350:357
msgctxt "TipBumpFreq"
msgid "Frequency of additional bumps on surface (distance between them)."
msgstr "Frequenza di bump aggiuntivi sulla superficie (distanza tra loro)."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3163 ..Editor.Vegetation.Surface.Surface.Text :3197
#: data/gui/Game_Tweak.layout:338 ..Tweak.Surface.Text :345:362:369
msgctxt "TipBumpAmp"
msgid "Amplitude of additional bumps on surface (how high)."
msgstr "Ampiezza di bump aggiuntivi sulla superficie (quanto intensa)."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3148 ..Editor.Vegetation.Surface.Surface.Text
#: :3155..Editor.Vegetation.Surface.Surface.Combo
#: :3168..Editor.Vegetation.Surface.Surface.Text :3172
#: data/gui/Game_Tweak.layout:404 ..Tweak.Surface.Text
msgctxt "TipSurfType"
msgid "Type of surface, determines sound."
msgstr "Tipo di superficie, determina il suono."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3183 ..Editor.Vegetation.Surface.Surface.Text :3207
#: data/gui/Game_Tweak.layout:374 ..Tweak.Surface.Text :381
msgctxt "TipRollDrag"
msgid ""
"Drag for wheels, bigger will make them slow down faster (eg. 140 for snow, "
"80 for grass)."
msgstr ""
"Ruote di trascinamento, più grandi li farà rallentare più velocemente (es. "
"140 per neve, 80 per erba)."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3187 ..Editor.Vegetation.Surface.Surface.Text :3202
#: data/gui/Game_Tweak.layout:288 ..Tweak.Surface.Text :295:300:307:312:319
msgctxt "TipSurfFrict"
msgid "Surface friction (eg. 1.0 asphalt, 0.7 gravel, 0.5 ice)."
msgstr "Attrito di superficie (es. 1.0 asfalto, 0.7 ghiaia, 0.5 ghiaccio)."

#. TIP  Vegetation 
#: data/gui/Editor.layout:1878 ..Editor.Grasses.Properties.Text
#: :1882..Editor.Grasses.Properties.Slider
#: :1888..Editor.Grasses.Properties.Edit
msgctxt "TipGrassDens"
msgid "How dense/rare will the grass be on terrain."
msgstr "Quanto fitta/scarsa sarà l'erba presente sul terreno."

#. TIP  Vegetation 
#: data/gui/Editor.layout:2325 ..Editor.Vegetation.Properties.Text
#: :2329..Editor.Vegetation.Properties.Slider
#: :2335..Editor.Vegetation.Properties.Edit
msgctxt "TipTreeDens"
msgid "How many models (trees, rocks, etc.) will be on track."
msgstr "Quanti modelli (alberi, rocce ecc.) saranno presenti su pista."

#. TIP  Vegetation 
#: data/gui/Editor.layout:2341 ..Editor.Vegetation.Properties.Text
#: :2345..Editor.Vegetation.Properties.Slider
#: :2350..Editor.Vegetation.Properties.Edit
msgctxt "TipModelRoadDist"
msgid "Distance from road to models (global, added to each model's own value)."
msgstr ""
"Distanza dei modelli dalla strada (globale, aggiunto ad ogni valore del "
"modello)."

#. TIP  Vegetation 
msgctxt "TipImpDist"
msgid "Far distance at which the 2d impostors dissapear."
msgstr "Lontananza in cui gli impostori in 2d scompaiono."

#. TIP  Vegetation  page,vis 
msgctxt "TipGrassPageLength"
msgid ""
"Grass Page length (low will reload often, high will cause visible switching)."
msgstr ""
"Lunghezza pagine erba (bassa si ricarica spesso, alta causa commutazione "
"visibile)."

#. TIP  Vegetation  page,vis 
msgctxt "TipModelPageLength"
msgid ""
"Models Page length (low will reload often and make too many batches, high "
"will cause visible switching and make too many drawn triangles)."
msgstr ""
"Lunghezza pagina modelli (se bassa si ricarica spesso e crea molti batch, "
"alta causa commutazione visibile e crea troppi triangoli disegnati)."

#. TIP  Vegetation  page,vis 
msgctxt "TipGrassVisDist"
msgid "Visibility distance for grass pages."
msgstr "Distanza di visibilità per le pagine di erba."

#. TIP  Vegetation  page,vis 
msgctxt "TipModelVisDist"
msgid ""
"Visibility distance for 3d models, after this they turn into 2d impostors."
msgstr ""
"Distanza di visibilità per i modelli in 3d, dopo l'applicazione si "
"trasformano in impostori in 2d."

#. TIP  Grass 
#: data/gui/Editor.layout:1894 ..Editor.Grasses.Properties.Text
#: :1898..Editor.Grasses.Properties.Slider
#: :1903..Editor.Grasses.Properties.Edit
msgctxt "TipGrDensSmooth"
msgid ""
"Grass density map - smooth size. Higher will make a smoother transition from "
"road to grass."
msgstr ""
"Densità erba su mappa - dimensione smoothing. Un valore più alto creerà una "
"transizione più liscia dalla strada all'erba."

#. TIP  Grass 
#: data/gui/Editor.layout:1999 ..Editor.Grasses.Grasses.Text
#: :2003..Editor.Grasses.Grasses.Slider :2008..Editor.Grasses.Grasses.Text
#: :2018..Editor.Grasses.Grasses.Slider :2046:2061
msgctxt "TipGrassPatchLengthMin"
msgid "Minimal grass patch length."
msgstr "Altezza minima di patch erba."

#. TIP  Grass 
#: data/gui/Editor.layout:2014 ..Editor.Grasses.Grasses.Text :2023
msgctxt "TipGrassPatchLengthMax"
msgid "Maximal grass patch length."
msgstr "Lunghezza massima di patch erba."

#. TIP  Grass 
#: data/gui/Editor.layout:2042 ..Editor.Grasses.Grasses.Text :2051
msgctxt "TipGrassPatchHeightMin"
msgid "Minimal grass patch height."
msgstr "Altezza minima di patch erba."

#. TIP  Grass 
#: data/gui/Editor.layout:2057 ..Editor.Grasses.Grasses.Text :2066
msgctxt "TipGrassPatchHeightMax"
msgid "Maximal grass patch height."
msgstr "Altezza massima di patch erba."

#. TIP  Grass  sway 
msgctxt "TipGrassSwayLen"
msgid "Grass sway effect length (scale)."
msgstr "Lunghezza ondeggiamento dell'erba (scala)."

#. TIP  Grass  sway 
msgctxt "TipGrassSwaySpd"
msgid "Grass sway effect speed."
msgstr "Velocità ondeggiamento dell'erba."

#. TIP  Grass  sway 
msgctxt "TipGrassSwayDistr"
msgid "Grass sway effect distribution (how random)."
msgstr "Distribuzione dell'effetto ondeggiamento dell'erba (casuale)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2164 ..Editor.Grasses.Channels.Text
#: :2169..Editor.Grasses.Channels.Edit :2173..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMaxAngle"
msgid ""
"Terrain angle above which there won't be any grass (starts fading out, for "
"smooth angle)."
msgstr ""
"Angolo di terreno posto sopra dove non ci sarà alcun strato di erba (inizio "
"dissolvenza, per angolo smoothing)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2194 ..Editor.Grasses.Channels.Text
#: :2199..Editor.Grasses.Channels.Edit :2203..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMinHeight"
msgid ""
"Terrain height below which there won't be any grass (starts fading out, for "
"smooth distance)."
msgstr ""
"Altezza terreno posto sotto in cui non ci sarà erba (inizio dissolvenza, per "
"distanza smoothing)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2209 ..Editor.Grasses.Channels.Text
#: :2214..Editor.Grasses.Channels.Edit :2218..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMaxHeight"
msgid ""
"Terrain height above which there won't be any grass (starts fading out, for "
"smooth distance)."
msgstr ""
"Angolo di terreno posto sopra dove non ci sarà alcun strato di erba (inizio "
"dissolvenza, per distanza smoothing)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2072 ..Editor.Grasses.Grasses.Text
#: :2079..Editor.Grasses.Grasses.Button
msgctxt "TipGrassMaterial"
msgid "Grass material (type) for this layer, chosen to match track's scenery."
msgstr ""
"(tipo) Materiale erba per layer, scelto per abbinare lo scenario della pista."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1973 ..Editor.Grasses.Grasses.Text
msgctxt "TipGrassColorMap"
msgid ""
"Grass colormap for scenery, usually same choice as for grass material. "
"Additional grass coloring."
msgstr ""
"Colormap erba per layer, di solito stessa scelta come i materiali erba. "
"Colorazione erba aggiuntivo."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1955 ..Editor.Grasses.Grasses.ProgressBar
#: :1989..Editor.Grasses.Grasses.Text
#: :2138..Editor.Grasses.Channels.ProgressBar
msgctxt "TipGrassLayers"
msgid "Grass layers. Recommended use 1-4."
msgstr "Layer erba. Si raccomanda di usare da 1 a 4."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1967 ..Editor.Grasses.Grasses.Image
msgctxt "TipGrassPrev"
msgid "Preview image of this grass layer (changed with material)."
msgstr "Anteprima immagine del layer d'erba (cambiato con materiale)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1984 ..Editor.Grasses.Grasses.Check
msgctxt "TipToggleGrass"
msgid "Enable or disable this grass layer."
msgstr "Attiva o disattiva il layer erba."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2085 ..Editor.Grasses.Grasses.Text
#: :2089:2093..Editor.Grasses.Grasses.Slider :2099..Editor.Grasses.Grasses.Text
#: :2103:2107..Editor.Grasses.Grasses.Slider
msgctxt "TipGrassLayerDens"
msgid "Grass density for layer (how often it will appear)."
msgstr "Densità erba per layer (quanto spesso si manifesta)."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:1922 ..Editor.Grasses.Grasses.TabC
#: :2368..Editor.Vegetation.Models.TabC
#: :2450..Editor.Vegetation.Models.ProgressBar
#: :2488..Editor.Vegetation.Models.Text
msgctxt "TipVegetGroups"
msgid "Vegetation model groups (recommended use 4-6), affects performance."
msgstr ""
"Gruppi di modello vegetazione (si raccomanda di usare da 4 a 6), influisce "
"sulle prestazioni."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2456 ..Editor.Vegetation.Models.Text
#: :2463..Editor.Vegetation.Models.Button
msgctxt "TipVegetModels"
msgid "Models (meshes) for vegetation. Found in data/trees/."
msgstr "Modelli (mesh) per vegetazione. Locata in data/trees/."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2468 ..Editor.Vegetation.Models.Text
#: :2472:2476..Editor.Vegetation.Models.Slider
msgctxt "TipModelDens"
msgid "How often model will appear on track area."
msgstr "Quanto spesso il modello apparirà sulla zona della pista."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2483 ..Editor.Vegetation.Models.Check
msgctxt "TipToggleModel"
msgid "Enable or disable this model."
msgstr "Attiva/disattiva il modello."

#. TIP  Vegetation  models 
msgctxt "TipRoadDist"
msgid "How far from road will the model appear."
msgstr "Quanto distante dalla strada verrà visualizzato il modello."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2522 ..Editor.Vegetation.Models.Text
#: :2531:2539..Editor.Vegetation.Models.Slider
msgctxt "TipModelMinScale"
msgid "Model minimal scale."
msgstr "Scala minima del modello."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2527 ..Editor.Vegetation.Models.Text
#: :2535:2543..Editor.Vegetation.Models.Slider
msgctxt "TipModelMaxScale"
msgid "Model maximal scale."
msgstr "Scala massima del modello."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2240 ..Editor.Grasses.Channels.Text
#: :2244:2248..Editor.Grasses.Channels.Slider
#: :2549..Editor.Vegetation.Models.Text
#: :2553:2557..Editor.Vegetation.Models.Slider
msgctxt "TipRoadDist2"
msgid "Additional distance to road."
msgstr "Distanza dalla strada aggiuntiva."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2562 ..Editor.Vegetation.Models.Text
msgctxt "TipRoadDistMax"
msgid ""
"Maximum distance to road. E.g. can be used for vegetation that should appear "
"only close to road: plants, bushes etc."
msgstr ""
"Distanza massima dalla strada. Es. può essere usata per la vegetazione che "
"deve apparire solo vicino alla strada: piante, cespugli ecc."

#. TIP  Vegetation  models 
msgctxt "TipWindInflL"
msgid "Wind influence on model, length scale."
msgstr "Influenza del vento sul modello, scala di lunghezza."

#. TIP  Vegetation  models 
msgctxt "TipWindInflH"
msgid "Wind influence on model, height scale."
msgstr "Influenza del vento sul modello, scala di altezza."

#. TIP  Vegetation  models 
msgctxt "TipModelPrev"
msgid "Model preview image."
msgstr "Anteprima immagine del modello."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2574 ..Editor.Vegetation.Models.Text
#: :2578:2582..Editor.Vegetation.Models.Slider
msgctxt "TipLTrMaxTerAng"
msgid "Terrain angle above which model won't appear."
msgstr "Angolo di terreno posto sopra dove il modello non apparirà."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2587 ..Editor.Vegetation.Models.Text
#: :2592..Editor.Vegetation.Models.Edit :2596..Editor.Vegetation.Models.Slider
msgctxt "TipLTrMinTerH"
msgid "Terrain height below which model won't appear."
msgstr "Altezza di terreno posta sotto in cui il modello non apparirà."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2601 ..Editor.Vegetation.Models.Text
#: :2606..Editor.Vegetation.Models.Edit :2610..Editor.Vegetation.Models.Slider
msgctxt "TipLTrMaxTerH"
msgid "Terrain height above which model won't appear."
msgstr "Altezza terreno posto sopra dove il modello non apparirà."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2615 ..Editor.Vegetation.Models.Text
#: :2620..Editor.Vegetation.Models.Edit :2624..Editor.Vegetation.Models.Slider
msgctxt "TipLTrFlDepth"
msgid "Maximal depth in fluid allowed (set 0 to exclude from fluids)."
msgstr ""
"Profondità massima nel fluido consentito (imposta 0 per escludere dai "
"fluidi)."

