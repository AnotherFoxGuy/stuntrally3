#: :3168..Editor.Vegetation.Surface.Surface.Text :3172
#: data/gui/Game_Tweak.layout:404 ..Tweak.Surface.Text
msgctxt "TipSurfType"
msgid "Type of surface, determines sound."
msgstr "Type de surface, détermine le son."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3183 ..Editor.Vegetation.Surface.Surface.Text :3207
#: data/gui/Game_Tweak.layout:374 ..Tweak.Surface.Text :381
msgctxt "TipRollDrag"
msgid ""
"Drag for wheels, bigger will make them slow down faster (eg. 140 for snow, "
"80 for grass)."
msgstr ""
"Friction des roues, une valeur plus élevée réduit la vitesse (exemple : 140 "
"pour la neige, 80 pour l'herbe)."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3187 ..Editor.Vegetation.Surface.Surface.Text :3202
#: data/gui/Game_Tweak.layout:288 ..Tweak.Surface.Text :295:300:307:312:319
msgctxt "TipSurfFrict"
msgid "Surface friction (eg. 1.0 asphalt, 0.7 gravel, 0.5 ice)."
msgstr ""
"Friction de la surface (exemple : 1.0 pour l'asphalte, 0.7 pour le gravier, "
"0.5 pour la glace)."

#. TIP  Vegetation 
#: data/gui/Editor.layout:1878 ..Editor.Grasses.Properties.Text
#: :1882..Editor.Grasses.Properties.Slider
#: :1888..Editor.Grasses.Properties.Edit
msgctxt "TipGrassDens"
msgid "How dense/rare will the grass be on terrain."
msgstr "À quelle fréquence l'herbe apparaîtra sur le terrain."

#. TIP  Vegetation 
#: data/gui/Editor.layout:2325 ..Editor.Vegetation.Properties.Text
#: :2329..Editor.Vegetation.Properties.Slider
#: :2335..Editor.Vegetation.Properties.Edit
msgctxt "TipTreeDens"
msgid "How many models (trees, rocks, etc.) will be on track."
msgstr "Combien d'objets (arbres, rochers, etc.) seront sur la piste."

#. TIP  Vegetation 
#: data/gui/Editor.layout:2341 ..Editor.Vegetation.Properties.Text
#: :2345..Editor.Vegetation.Properties.Slider
#: :2350..Editor.Vegetation.Properties.Edit
msgctxt "TipModelRoadDist"
msgid "Distance from road to models (global, added to each model's own value)."
msgstr ""
"Distance de la route aux modèles (globale, ajoutée à la valeur de chaque "
"modèle)."

#. TIP  Vegetation 
msgctxt "TipImpDist"
msgid "Far distance at which the 2d impostors dissapear."
msgstr "Distance à laquelle les sprites disparaissent."

#. TIP  Vegetation  page,vis 
msgctxt "TipGrassPageLength"
msgid ""
"Grass Page length (low will reload often, high will cause visible switching)."
msgstr ""
"Longueur de la page d'herbe (une valeur basse rechargera souvent, une valeur "
"élevée peut causer des problèmes d'affichage)"

#. TIP  Vegetation  page,vis 
msgctxt "TipModelPageLength"
msgid ""
"Models Page length (low will reload often and make too many batches, high "
"will cause visible switching and make too many drawn triangles)."
msgstr ""

#. TIP  Vegetation  page,vis 
msgctxt "TipGrassVisDist"
msgid "Visibility distance for grass pages."
msgstr "Distance de vue de l'herbe."

#. TIP  Vegetation  page,vis 
msgctxt "TipModelVisDist"
msgid ""
"Visibility distance for 3d models, after this they turn into 2d impostors."
msgstr ""
"Distance de vue pour les modèles 3D, après cette distance ils se "
"transforment en sprites 2D."

#. TIP  Grass 
#: data/gui/Editor.layout:1894 ..Editor.Grasses.Properties.Text
#: :1898..Editor.Grasses.Properties.Slider
#: :1903..Editor.Grasses.Properties.Edit
msgctxt "TipGrDensSmooth"
msgid ""
"Grass density map - smooth size. Higher will make a smoother transition from "
"road to grass."
msgstr ""
"Carte de la densité d'herbe - adoucissement. Une valeur plus élevée rendra "
"la transition entre la route et l'herbe plus douce."

#. TIP  Grass 
#: data/gui/Editor.layout:1999 ..Editor.Grasses.Grasses.Text
#: :2003..Editor.Grasses.Grasses.Slider :2008..Editor.Grasses.Grasses.Text
#: :2018..Editor.Grasses.Grasses.Slider :2046:2061
msgctxt "TipGrassPatchLengthMin"
msgid "Minimal grass patch length."
msgstr "Longueur minimale de l'herbe."

#. TIP  Grass 
#: data/gui/Editor.layout:2014 ..Editor.Grasses.Grasses.Text :2023
msgctxt "TipGrassPatchLengthMax"
msgid "Maximal grass patch length."
msgstr "Longueur maximale de l'herbe."

#. TIP  Grass 
#: data/gui/Editor.layout:2042 ..Editor.Grasses.Grasses.Text :2051
msgctxt "TipGrassPatchHeightMin"
msgid "Minimal grass patch height."
msgstr "Hauteur minimale de l'herbe."

#. TIP  Grass 
#: data/gui/Editor.layout:2057 ..Editor.Grasses.Grasses.Text :2066
msgctxt "TipGrassPatchHeightMax"
msgid "Maximal grass patch height."
msgstr "Hauteur maximale de l'herbe."

#. TIP  Grass  sway 
msgctxt "TipGrassSwayLen"
msgid "Grass sway effect length (scale)."
msgstr "Intensité du \"vent\" sur l'herbe."

#. TIP  Grass  sway 
msgctxt "TipGrassSwaySpd"
msgid "Grass sway effect speed."
msgstr "Vitesse du \"vent\" sur l'herbe."

#. TIP  Grass  sway 
msgctxt "TipGrassSwayDistr"
msgid "Grass sway effect distribution (how random)."
msgstr "Distribution de l'effet de \"vent\" sur l'herbe."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2164 ..Editor.Grasses.Channels.Text
#: :2169..Editor.Grasses.Channels.Edit :2173..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMaxAngle"
msgid ""
"Terrain angle above which there won't be any grass (starts fading out, for "
"smooth angle)."
msgstr "Pente maximale du terrain pour qu'il ait de l'herbe dessus."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2194 ..Editor.Grasses.Channels.Text
#: :2199..Editor.Grasses.Channels.Edit :2203..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMinHeight"
msgid ""
"Terrain height below which there won't be any grass (starts fading out, for "
"smooth distance)."
msgstr "En-dessous de cette altitude, il n'y aura pas (ou moins) d'herbe."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2209 ..Editor.Grasses.Channels.Text
#: :2214..Editor.Grasses.Channels.Edit :2218..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMaxHeight"
msgid ""
"Terrain height above which there won't be any grass (starts fading out, for "
"smooth distance)."
msgstr "Au-delà de cette altitude, il n'y aura pas (ou moins) d'herbe."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2072 ..Editor.Grasses.Grasses.Text
#: :2079..Editor.Grasses.Grasses.Button
msgctxt "TipGrassMaterial"
msgid "Grass material (type) for this layer, chosen to match track's scenery."
msgstr ""
"Matériau (type) d'herbe pour ce calque, choisi pour correspondre au paysage "
"du circuit."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1973 ..Editor.Grasses.Grasses.Text
msgctxt "TipGrassColorMap"
msgid ""
"Grass colormap for scenery, usually same choice as for grass material. "
"Additional grass coloring."
msgstr ""
"Carte de couleur de l'herbe pour le paysage, généralement le même choix que "
"pour le matériau d'herbe. Permet de changer la couleur de l'herbe."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1955 ..Editor.Grasses.Grasses.ProgressBar
#: :1989..Editor.Grasses.Grasses.Text
#: :2138..Editor.Grasses.Channels.ProgressBar
msgctxt "TipGrassLayers"
msgid "Grass layers. Recommended use 1-4."
msgstr "Calques d'herbe. Des valeurs entre 1 et 4 sont recommandées."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1967 ..Editor.Grasses.Grasses.Image
msgctxt "TipGrassPrev"
msgid "Preview image of this grass layer (changed with material)."
msgstr "Image d'aperçu de ce calque d'herbe (changé avec le matériau)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1984 ..Editor.Grasses.Grasses.Check
msgctxt "TipToggleGrass"
msgid "Enable or disable this grass layer."
msgstr "Activer ou désactiver ce calque d'herbe."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2085 ..Editor.Grasses.Grasses.Text
#: :2089:2093..Editor.Grasses.Grasses.Slider :2099..Editor.Grasses.Grasses.Text
#: :2103:2107..Editor.Grasses.Grasses.Slider
msgctxt "TipGrassLayerDens"
msgid "Grass density for layer (how often it will appear)."
msgstr "Densité de l'herbe pour ce calque."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:1922 ..Editor.Grasses.Grasses.TabC
#: :2368..Editor.Vegetation.Models.TabC
#: :2450..Editor.Vegetation.Models.ProgressBar
#: :2488..Editor.Vegetation.Models.Text
msgctxt "TipVegetGroups"
msgid "Vegetation model groups (recommended use 4-6), affects performance."
msgstr ""
"Groupes de modèles de végétation (4 à 6 sont recommandés), affecte la "
"performance."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2456 ..Editor.Vegetation.Models.Text
#: :2463..Editor.Vegetation.Models.Button
msgctxt "TipVegetModels"
msgid "Models (meshes) for vegetation. Found in data/trees/."
msgstr "Modèles (maillages) pour la végétation, trouvés dans data/trees/."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2468 ..Editor.Vegetation.Models.Text
#: :2472:2476..Editor.Vegetation.Models.Slider
msgctxt "TipModelDens"
msgid "How often model will appear on track area."
msgstr "À quelle fréquence le modèle apparaîtra sur le circuit."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2483 ..Editor.Vegetation.Models.Check
msgctxt "TipToggleModel"
msgid "Enable or disable this model."
msgstr "Activer ou desactiver ce modèle"

#. TIP  Vegetation  models 
msgctxt "TipRoadDist"
msgid "How far from road will the model appear."
msgstr "À quelle distance de la route le modèle commencera à apparaître."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2522 ..Editor.Vegetation.Models.Text
#: :2531:2539..Editor.Vegetation.Models.Slider
msgctxt "TipModelMinScale"
msgid "Model minimal scale."
msgstr "Taille minimum du modèle."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2527 ..Editor.Vegetation.Models.Text
#: :2535:2543..Editor.Vegetation.Models.Slider
msgctxt "TipModelMaxScale"
msgid "Model maximal scale."
msgstr "Taille maximale du modèle."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2240 ..Editor.Grasses.Channels.Text
#: :2244:2248..Editor.Grasses.Channels.Slider
#: :2549..Editor.Vegetation.Models.Text
#: :2553:2557..Editor.Vegetation.Models.Slider
msgctxt "TipRoadDist2"
msgid "Additional distance to road."
msgstr "Distance additionnelle à la route"

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2562 ..Editor.Vegetation.Models.Text
msgctxt "TipRoadDistMax"
msgid ""
"Maximum distance to road. E.g. can be used for vegetation that should appear "
"only close to road: plants, bushes etc."
msgstr ""
"Distance maximale à la route. Peut être utilisé pour la végétation qui ne "
"devrait apparaître qu'à proximité à la route : plantes, buissons, etc..."

#. TIP  Vegetation  models 
msgctxt "TipWindInflL"
msgid "Wind influence on model, length scale."
msgstr "Influence du vent sur le modèle (dans le sens de la largeur)."

#. TIP  Vegetation  models 
msgctxt "TipWindInflH"
msgid "Wind influence on model, height scale."
msgstr "Influence du vent sur le modèle (dans le sens de la hauteur)."

#. TIP  Vegetation  models 
msgctxt "TipModelPrev"
msgid "Model preview image."
msgstr "Image d'aperçu du modèle."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2574 ..Editor.Vegetation.Models.Text
#: :2578:2582..Editor.Vegetation.Models.Slider
msgctxt "TipLTrMaxTerAng"
msgid "Terrain angle above which model won't appear."
msgstr "Au-delà de cet angle du terrain, le modèle n'apparaîtra pas."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2587 ..Editor.Vegetation.Models.Text
#: :2592..Editor.Vegetation.Models.Edit :2596..Editor.Vegetation.Models.Slider
msgctxt "TipLTrMinTerH"
msgid "Terrain height below which model won't appear."
msgstr "En-dessous de cette altitude, le modèle n'apparaîtra pas."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2601 ..Editor.Vegetation.Models.Text
#: :2606..Editor.Vegetation.Models.Edit :2610..Editor.Vegetation.Models.Slider
msgctxt "TipLTrMaxTerH"
msgid "Terrain height above which model won't appear."
msgstr "Au-delà de cette altitude, le modèle n'apparaîtra pas."

#. TIP  Vegetation  terrain 
#: data/gui/Editor.layout:2615 ..Editor.Vegetation.Models.Text
#: :2620..Editor.Vegetation.Models.Edit :2624..Editor.Vegetation.Models.Slider
msgctxt "TipLTrFlDepth"
msgid "Maximal depth in fluid allowed (set 0 to exclude from fluids)."
msgstr ""
"Profondeur maximale dans le liquide autorisée (définissez cette valeur à 0 "
"pour l'exclure des liquides)."

